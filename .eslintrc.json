{
  "extends": [ "next/core-web-vitals", "eslint:recommended" ],
  "rules" : {
    "no-console": "warn",
     "quotes": [
        "error",
        "single"
     ],
     "semi": "off",
    "indent": [
      "error",
      "2"
    ],
    "no-multi-spaces": "error",
    "space-in-brackets": ["error", "always"]
  }
}
